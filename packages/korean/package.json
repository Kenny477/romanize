{
  "name": "@romanize/korean",
  "version": "0.1.2",
  "description": "Romanize Korean Hangul",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/",
    "package.json"
  ],
  "scripts": {
    "prebuild": "npm run clean && tsc --noEmit",
    "build": "esbuild index.ts --bundle --minify --sourcemap --outdir=dist",
